{
  "name": "create-t3-ui",
  "version": "0.1.5",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cli": "turbo run build && node apps/cli/build/index.js"
  },
  "bin": "apps/cli/build/index.js",
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "execa": "^7.0.0",
    "fs-extra": "^11.1.0",
    "ora": "^6.1.2",
    "portfinder": "^1.0.32",
    "puppeteer": "^19.7.4",
    "zod": "^3.21.4"
  },
  "packageManager": "pnpm@7.27.1"
}
